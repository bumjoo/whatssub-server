{
  "name": "whatssub-backend",
  "version": "0.0.1",
  "description": "Graphql Yoga Typescript Example",
  "main": "src/server.ts",
  "repository": "https://github.com/dooboolab/whatssub-backend",
  "author": "dooboolab",
  "license": "MIT",
  "scripts": {
    "start": "NODE_ENV=development nodemon --exec ts-node src/server.ts",
    "migrate:create": "npx sequelize-cli migrate:generate",
    "deploy": "tsc && az webapp up -n whatssub-dev",
    "prod": "NODE_ENV=production ts-node src/server.ts",
    "watch": "nodemon --watch 'src/**/*.ts' --ignore 'src/**/*.spec.ts' --exec 'ts-node' src/index.ts",
    "lint": "eslint src/**/**/*.ts && eslint tests/**/**/*.ts",
    "tsc": "tsc",
    "test": "NODE_ENV=test jest",
    "test:coverage": "jest -w 1 --coverage",
    "test:watch": "jest --watch",
    "codegen": "graphql-codegen",
    "generate": "graphql-codegen --config codegen.yml",
    "codecov": "codecov"
  },
  "dependencies": {
    "apollo-server": "^2.8.1",
    "apollo-server-express": "^2.8.1",
    "cors": "^2.8.5",
    "graphql": "^14.4.2",
    "graphql-import": "^0.7.1",
    "http": "^0.0.0",
    "jest": "^24.8.0",
    "jsonwebtoken": "^8.5.1",
    "merge-graphql-schemas": "^1.7.0",
    "mysql2": "^1.6.5",
    "sequelize": "^5.12.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^1.4.0",
    "@graphql-codegen/typescript": "^1.4.0",
    "@graphql-codegen/typescript-resolvers": "1.4.0",
    "@types/bluebird": "^3.5.27",
    "@types/cors": "^2.8.5",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.17",
    "@types/jsonwebtoken": "^8.3.2",
    "@types/node": "^12.6.9",
    "@types/sequelize": "^4.28.4",
    "@types/validator": "^10.11.2",
    "@typescript-eslint/eslint-plugin": "^1.13.0",
    "@typescript-eslint/parser": "^1.13.0",
    "codecov": "^3.5.0",
    "dotenv": "^8.0.0",
    "eslint": "^6.1.0",
    "eslint-config-standard": "^13.0.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "nodemon": "^1.19.1",
    "sequelize-cli": "^5.5.0",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.3"
  },
  "resolutions": {
    "graphql": "14.4.2"
  }
}
